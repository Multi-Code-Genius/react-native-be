datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id           String   @id @default(uuid())
  email        String   @unique
  password     String
  name         String?
  profile_pic  String?
  dob          DateTime?     
  mobileNumber String? @unique 
  location     String?      
  status       String  @default("active") 
  createdAt    DateTime @default(now())
  updatedAt DateTime @updatedAt @default(now())

}

